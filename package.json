{
  "name": "api-wrongcars",
  "version": "0.0.1",
  "description": "",
  "author": "Stanislav Strugovshchikov <stanislav.stru@gmail.com>",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:seed": "ts-node prisma/seed/initialQuaries.ts",
    "prisma:sees:products": "ts-node prisma/seed/_products.ts",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "stripe:webhook-redirect": "stripe listen --forward-to localhost:8080/api/v1/payments/webhook --events checkout.session.completed,checkout.session.expired",
    "stripe:checkout-session-completed": "stripe trigger checkout.session.completed",
    "stripe:checkout-session-expired": "stripe trigger checkout.session.expired",
    "postinstall": "prisma generate",
    "vercel-build": "prisma migrate deploy && ts-node prisma/seed/initialWCOQuaries.ts",
    "generate-type": "swagger-typescript-api -p swagger.json -o types -n types-from-swagger.ts --no-client"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/swagger": "^8.0.7",
    "@prisma/client": "^6.1.0",
    "@types/multer": "^1.4.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "prisma-generator-typescript-interfaces": "^1.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "shippo": "^2.14.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@brakebein/prisma-generator-nestjs-dto": "^1.23.3",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vercel/blob": "^0.27.0",
    "decimal.js": "^10.4.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "handlebars": "^4.7.8",
    "jest": "^29.5.0",
    "moment": "^2.30.1",
    "nodemailer": "^6.10.0",
    "prettier": "^3.0.0",
    "prisma": "^6.1.0",
    "source-map-support": "^0.5.21",
    "stripe": "^17.5.0",
    "supertest": "^6.3.3",
    "swagger-typescript-api": "^13.0.23",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
